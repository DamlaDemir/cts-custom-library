{
  "name": "cts-custom-library",
  "version": "0.1.0",
  "private": false,
  "description": "This isn't particularly useful, it's used as a demo for how to publish Vue multi components to NPM!",
  "main": "./dist/CtsCustomLibrary.umd.js",
  "unpkg": "dist/CtsCustomLibrary.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:npm": "vue-cli-service build --target lib --name CtsCustomLibrary src/CtsCustomLibrary.js"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamlaDemir/cts-custom-library.git"
  },
  "author": "Damla Demir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DamlaDemir/cts-custom-library/issues"
  },
  "homepage": "https://github.com/DamlaDemir/cts-custom-library#readme",
  "dependencies": {
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "buefy": "^0.8.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.18.2",
    "vue-template-compiler": "^2.6.10"
  }
}
